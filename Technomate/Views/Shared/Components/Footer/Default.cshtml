@model Technomate.Models.Setting

<div class="col-lg-4 col-md-6 footer-about">
    <a href="@Url.Action("Index", "Home")" class="d-flex align-items-center">
        <span class="sitename">Technomate</span>
    </a>

    <div class="footer-contact pt-3">
        <p>@Model.Address</p>

        <p>
            <strong>Phone:</strong> @Model.Phone
        </p>

        <p>
            <strong>Email:</strong> @Model.Email
        </p>

        <p>
            <strong>Website:</strong>
            <a href="@Model.Website" target="_blank">@Model.Website</a>
        </p>
    </div>
</div>
